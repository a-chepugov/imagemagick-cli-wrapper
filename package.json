{
	"name": "imagemagick-cli-wrapper",
	"version": "2.1.0",
	"description": "",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"module": "build/index.js",
	"keywords": [
		"imagemagick",
		"typescript",
		"ts",
		"library",
		"toolkit",
		"utils",
		"cli"
	],
	"scripts": {
		"prepack": "npm install --production && npm run js:build",
		"js:build": "tsc",
		"js:delete": "rm -rf ./build",
		"js:build:clear": "npm run js:delete && npm run js:build;",
		"js:build:watch": "tsc --watch",
		"docs:build": "typedoc source/index.ts",
		"docs:delete": "rm -rf ./docs",
		"docs:build:clear": "npm run docs:delete && npm run docs:build;",
		"all:build": "npm run js:build:clear && npm run docs:build:clear;",
		"version:major": "npm version major",
		"version:minor": "npm version minor",
		"version:patch": "npm version patch",
		"preversion": "npm run test",
		"prepublishOnly": "npm run test && docs:build:clear",
		"test": "mocha -r ts-node/register source/**/*test.ts"
	},
	"devDependencies": {
		"@types/chai": "^4.2.12",
		"@types/mocha": "^5.2.7",
		"chai": "^4.2.0",
		"mocha": "^6.2.3",
		"typedoc": "^0.17.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/a-chepugov/imagemagick-cli-wrapper.git"
	},
	"optionalDependencies": {
		"@types/node": "^12.12.54",
		"typescript": "^3.9.7"
	},
	"author": "",
	"license": "ISC"
}
